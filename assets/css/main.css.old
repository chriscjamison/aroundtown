/* main.css */

body {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

#headr {
  width: 61.25em;
  height: 5.94em;
  margin: 0 auto;
  vertical-align: middle;
}

#headr > img {
  width: 5.76em;
  height: 4.69em;
	display: inline-block;
  margin: 0.6em 0;
}

h1 {
	font-family: "merriweather", serif;
	font-size: 2em;
	font-weight: 400;
	color: #323275;
	border: 1px solid #a5a29e;
  border-left: none;
	border-right: none;
}

#headr-showtimes {
	width: 21.25em !important;
	vertical-align: top;
}

#headr-showtimes h2 {
	width: 100%;
	border-bottom: 1px solid #a5a29e;
	font-family: "merriweather", serif;
	color: #323275;
}

#headr-showtimes p {
	margin-top: 0.1em;
	padding-top: 0.1em;
	font-family: "merriweather", serif;
	font-size: 0.78em;
	text-align: center;
}

.headr-main {
  width: initial;
  height: 1.05em;
  display: inline-block;
  margin: 0.5em auto 0.3em auto;
	padding: 0.1em 0 0.3em 0;
	vertical-align: top;
	text-align: center; 
}

#headr > input {
	width: 11.3em;
	display: inline-block;
	margin-top: 1em;
	vertical-align: top;
}

#headr > div {
  width: 17.9%;
  height: 3.5em;
  display: inline-block;
}

#menu {
  width: 100%;
  height: 2.8em;
	display: block;
	padding-top: 1.1em;
  background-color: #a68c6b;
}

#menu > a {
  display: block;
	float: left;
  font-family: "adelle-sans", "Franklin Gothic Medium", "Arial", "Helvetica", sans-serif;
	font-weight: 600;
	font-style: normal;
  text-transform: uppercase;
	text-align: center;
  color: #fff;
}

#menu > a:first-of-type {
	margin-left: 16%
}

#menu > span {
  width: 0.125em;
  height: 1em;
  display: block;
	float: left;
  margin: 0.25em 1.25em;
  background-color: #a5a29e;
}

#headr-shdow {
  width: 100%;
  height: 0.56em;
	position: absolute;
}

#headr-shdow > div {
	width: 11.6%;
	background: url('/aroundtown/assets/img/sep/menu_side_cap.png') 0 0 no-repeat;
	float: left;
}

#headr-shdow > div:nth-child(2) {
	width: 76.75%;
	background: url('/aroundtown/assets/img/sep/horiz_1366x9-fade_ltdark.png') 0 0 repeat-x;
}

#headr-shdow > div + div + div {
	background: url('/aroundtown/assets/img/sep/menu_side_cap-right.png') 0 0 no-repeat;
}

#bkgrnd {
	width: 100%;
	height: 1312px;
	position: absolute;
	top: 10em;
	left: 0;
	z-index: -1;
}

#bkgrnd-level_0 {
	width: 100%;
	height: 25.5em;
	background: none;
}

#bkgrnd-level_1 {
	width: 100%;
	height: 46.5em;
	background: url('/aroundtown/assets/img/bkgrnd/1.gif') 0 0 repeat-x;
}

#bkgrnd-level_2 {
	width: 100%;
	height: 40em;
	background: url('/aroundtown/assets/img/bkgrnd/2.png') 0 0 repeat-x;
}

#bkgrnd-level_3 {
	width: 100%;
	height: 33.5em;
	background: url('/aroundtown/assets/img/bkgrnd/3.jpg') 0 0 no-repeat;
}

#bkgrnd-level_4 {
	width: 100%;
	height: 49em;
	background-image: none;
	background-color: #fff;
}

#bkgrnd-footer {
	width: 100%;
	height: 20.3em;
	background: url('/aroundtown/assets/img/bkgrnd/footer-1.jpg') 0 0 no-repeat;
	background-color: #fff;
}

#bkgrnd-footer > div {
	height: 9px;
	background: url('/aroundtown/assets/img/sep/horiz_1366x9-fade_dark.png') 0 0 no-repeat;
}

#bkgrnd-footer > div + div  {
	width: 61.5em;
	height: 15em;
	margin: 4.75em auto 0 auto;
	background: none;
	opacity: 0.8;
}

#bkgrnd-footer > div + div > div {
	width: 16em;
	height: 15em;
	float: left;
	margin-right: 6.75em;
	background-color: #2f2f32;
}

#bkgrnd-footer > div + div > div + div + div {
	margin-right: 0;
}

#cntainr {
	width: 100%;
	height: 100%;
	margin: 0 auto;
}

#level_0 {
	width: 61.5em;
	height: 25.13em;
	margin: 0.5em auto 0 auto;
}

#box-l0-left {
	width: 38.1em;
	height: inherit;
	display: inline-block;
	vertical-align: top;
	font-family: "merriweather", serif;
	font-style: normal;
	font-weight: 400;
}

#box-l0-left > img {
	margin-left: 0.5em;
}

#box-l0-left > div {
	width: 38em;
	position: absolute;
	top: 29.25em;
}

h4 {
	width: 6.58em;
	margin: 0 auto;
	display: block;
	font-weight: 700;
}

.headr-level_0 {
	width: 5.45em;
	margin: 0.25em auto 0 auto;
	color: #013e6c;
}

#box-l0-left p {
	margin: 0.5em 0;
	font-size: 1em;
	text-align: center;
	color: #020276;
}

#box-l0-left > div > div + div > p {
	font-size: 1.15em;
	font-weight: 600;
	color: #a67737;
}

#box-l0-left > div > div {
	padding: 0.75em;
	background-color: #afc9de;
}

#box-l0-left > div > div + div {
	padding: 0.5em;
	background: none;
}

#box-l0-right {
	width: 23.13em;
	height: 25.13em;
	display: inline-block;
} 

#box-l0-right > div {
	width: 23.13em;
	height: 0.3em;
}

#box-l0-right > div + div {
	width: 21.6em;
	height: 10em;
	padding: 0.75em;
	background-color: #475b6b;
}	

#box-l0-right > div + div > div:first-of-type {
	width: 10em;
	height: 2px;
	margin: 0 auto;
	background-color: #737678;
}

h3 {
	width: 18.1em;
	display: block;
	margin: 0.25em auto;
	font-family: "merriweather", serif;
	font-size: 0.95em;
	font-weight: 400;
	text-align: center;
	color: #d2e4f3;	
}

#button-l0-contact_us {
	display: block;
	margin: 1em auto;
}

#level_1 {
	width: 100%;
	height: 43.5em;
	margin: 0 auto;
	padding: 1.5em 0;
	font-family: "merriweather", serif;
}

.headr-level_1 {
	width: 10.1em;
	display: block;
	margin: 0 auto;
}

#level_1 > img {
	width: 60em;
	height: 23.38em;
	display: block;
	margin: 1.5em auto;
}

#level_1 > p {
	margin: 1em auto;
	display: block;
	width: 60em;
	font-weight: 400;
}

#level_2 {
	width: 61.5em;
	height: 37em;
	margin: 0 auto;
	padding: 1.5em 0;
}

.headr-level_2 {
	width: 6.93em;
	display: block;
	margin: 0 auto 0.65em auto;
}

#box-l2-left, 
#box-l2-right {
	width: 32.5em;
	height: 33.13em;
	display: inline-block;
	font-family: "merriweather", serif;
}

#box-l2-left {
	vertical-align: top;
}

#box-l2-right {
	width: 17.5em;
}

#box-l2-left > a,
#box-l2-right > a {
	display: block;
	margin: 0.2em auto;
	font-family: "adelle-sans", "Franklin Gothic Medium", "Arial", "Helvetica", sans-serif;
	font-size: 1.2em;
	text-align: center;
	color: #a68b37;
}

#box-l2-left > p,
#box-l2-right > p {
	display: block;
  margin-top: 0.5em;
	font-size: 0.95em;
  text-align: center;
	color: #66686a;
}

#box-l2-left > div, 
#box-l2-right > div {
	width: 100%;
	height: 1px;
	margin: 0.5em 0;
	background-color: #a5a29e;
}

#box-l2-left > iframe + a + p + div + p {
	font-size: 0.95em;
	line-height: 1.4em;
	text-align: left;
}

#box-l2-left > a + span + div + p + div + a {
	font-size: 1em;
}

#box-l2-mid {
	width: 1px;
	height: 25.13em;
	display: inline-block;
	margin: 4em 5.45em;
	vertical-align: top;
	background-color: #a5a39e;
}

#box-l2-right > div {
	margin: 1.25em 0 1.5em 0;
}

#level_3 {
	width: 61.5em;
	height: 33.5em;
	margin: 0 auto;
	padding-top: 1.5em;
}

.headr-level_3 {
	width: 10.95em;
	display: block;
	margin: 0 auto 0.65em auto;
}

#level_3 > p {
	font-family: "merriweather", serif;
	font-size: 0.95em;
	line-height: 1.4em;
	color: #fff;
}

#level_3 > form > div {
	width: 53.25em;
	height: 20.25em;
	margin: 0 auto;
}

#level_3 > form > div > p {
	margin-top: 0.75em;
	padding-top: 0.75em;
	border-top: 1px solid #a5a29e;
	margin-top: 0.75em;
	padding-top: 0.75em;
	font-family: "merriweather";
	font-size: 1.25em;
	font-weight: 600;
	text-align: center;
	color: #fff;
}

input[type="button"], 
input[type="submit"] {
	width: 10.3em;
	height: 3.75em;
	padding: 0.75em;
	background-color: #e4c877;
	border: none;
	font-family: "adelle-sans";
	font-size: 1em;
	font-weight: 600;
	color: #a67737;
	box-shadow: 2px 2px 1px #66686a;
}

input[type=text] {
	display: inline-block;
	margin-bottom: 1.25em;
	padding: 0.5em 1.5em 0.6em 1em;
	border: 1px solid #a5a29e;
	font-family: "adelle-sans", sans-serif;
	font-size: 1.05em;
	color: #a5a29e;
}

input[type=submit] {
	position: relative;
	display: block;
	float: right;
	top: 0.75em;
	right: 4.5em;
}

textarea {
	display: block;
	padding: 0.6em 0.85em;
	border: 1px solid #a5a29e;
	font-family: "adelle-sans", sans-serif;
	font-size: 1.05em;
	color: #a5a29e;
}

#level_4 {
	width: 61.5em;
	height: 46em;
	margin: 0 auto;
	padding-top: 1.5em;
}

.headr-level_4 {
	width: 6.3em;
	display: block;
	margin: 0 auto 0.65em auto;
}

#level_4 > div {
	width: 16.25em;
	height: 21.25em;
	display: inline-block;
	margin-right: 6.1em;
	background-image: url('/aroundtown/assets/img/elements/social_media_icons.png');
}

#level_4 > div > span {
	display: block;
	margin: 17em auto 0 auto;
	font-family: "merriweather";
}

#level_4 > div > a {
	display: block;
	margin: 0 auto;
	font-family: "adelle-sans";
	color: #a58b37;
}

#social_media-facebook > span {
	width: 4.63em;
}

#social_media-facebook > a {
	width: 9.1em;
}

#social_media-twitter {
	margin-left: 11.25em;
	background-position: 0px -305px;
}

#social_media-twitter > span {
	width: 3.7em;
}

#social_media-twitter > a, 
#social_media-instagram > a {
	width: 6.35em;
}

#social_media-instagram {
	margin-right: 0 !important;
	background-position: 0px -680px;
}

#social_media-instagram > span {
	width: 5.2em;
}

#social_media-youtube {
	background-position: 0px -995px;
}

#social_media-youtube > span {
	width: 4.35em;
}

#social_media-youtube > a {
	width: 12.1em;
}

#social_media-periscope {
	background-position: 0px -1350px;
}

#social_media-periscope > span {
	width: 4.8em;
}

#social_media-periscope > a {
	width: 8.7em;
}

#footer {
	width: 61.5em;
	height: 20.3em;
	margin: 0 auto;
	z-index: 0;
}

#footer > img {
	display: block;
	padding: 0.8em 0;
}

#footer > div {
	width: 16em;
	height: 15em;
	margin-right: 6.75em;
	float: left;
}

#footer > div + div + div {
	margin-right: 0;
}

#footer > div > a {
	display: block;
	margin: 1.2em 0 1.35em 1.25em;
	font-family: "adelle-sans";
	color: #a58b37;
}

#footer > div + div > div {
	width: 1.88em;
	height: 1.88em;
	display: inline-block;
	margin: 0 0.5em 0.65em 1.25em;
	background-image: url('/aroundtown/assets/img/elements/social_media_icons-small.png');
}

#footer > div + div > div:first-of-type {
	margin-top: 1.05em;
}

#footer > div + div > div + a + div {
	background-position: 0px -155px;
}

#footer > div + div > div + a + div + a + div {
	background-position: 0px -80px;
}

#footer > div + div > div + a + div + a + div + a + div {
	background-position: 0px -35px;
}

#footer > div + div > div + a + div + a + div + a + div + a + div {
	background-position: 0px -115px;
}

#footer > div + div > a {
	display: inline-block;
	margin: 0.25em 6.5em 0.25em 0;
	vertical-align: top;
}

#footer > div + div > a:first-of-type {
	margin-top: 1.25em;
}

#footer > div + div + div > img {
	display: block;
	margin: 1.25em auto 0.5em auto;
	width: 165px;
}

#footer > div > span {
	display: block;
	margin-bottom: 0.35em;
	padding-right: 3.75em;
	font-family: "merriweather";
	font-size: 0.75em;
	text-align: right;
	color: #b7b7b7;
}
